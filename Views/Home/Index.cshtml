@{
    ViewData["Title"] = "Home Page";
}

@section css {
    <link href="@Url.Content("~/css/indexstyle.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/css/nav.css")" rel="stylesheet" type="text/css" />
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
}

<div id="homecontainer">
<header>
  <img src="../images/white-nosub-inflogo.png" alt="logo" class="white-logo">
  <input type="checkbox" id="nav-toggle" class="nav-toggle">
  <nav>
      <ul>
      <li><a href="/dashboard">Home</a></li>
      <li><a href="http://infinterest.com/#home">About</a></li>
      <li><a class="" href="vendor-registration">Vendor Sign Up</a></li>
      <li><a class="" href="broker-registration">Broker Sign Up</a></li>
      <li><a class="login">Log In</a></li>
      </ul>
  </nav>
  <label for="nav-toggle" class="nav-toggle-label">
      <span></span>
  </label>
</header>
    <div class="container wrapper">
        <div class="searchbox">
            <h4>Search Upcoming Open House Events.</h4>
            <form>
                <div class="form-group whereinput">
                  <label for="zipcode">Where</label>
                  <input type="text" class="form-control"placeholder="Zip Code">
                </div>
                <div class="form-group">
                    <label for="zipcode" class="when">When</label>
                    <input type="date" class="form-control daterange">
                    <label>to</label>
                    <input type="date" class="form-control daterange">
                </div>
                <div class="form-group">
                    <label for="price" class="when">Price Range</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="10000">
                    <label>to</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="100000000">
                </div>
                <a href="/search" class="btn btn-primary" type="submit">Search</a>
            </form>
        </div>
        <div class="loginbox">
            <form onsubmit="gotodashboard()">
                <i class="fas fa-times-circle"></i>
                <div class="form-group logformgroup">
                  <input type="text" class="form-control logformcon" placeholder="Email">
                </div>
                <div class="form-group logformgroup">
                  <input type="text" class="form-control logformcon" placeholder="Password">
                </div>
                <a class="btn btn-primary logbutton" href="dashboard">Log in</a>
            </form>
        </div>
    </div>
    </div>
</div>

    