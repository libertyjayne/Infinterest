        <div class="searchbox">
            <h4>Search Upcoming Open House Events.</h4>
            <form>
                <div class="form-group whereinput">
                  <label for="zipcode">Where</label>
                  <input type="text" class="form-control" placeholder="Zip Code" value="98007">
                </div>
                <div class="form-group">
                    <label for="zipcode" class="when">When</label>
                    <input type="date" class="form-control daterange" value="2018-01-01">
                    <label>to</label>
                    <input type="date" class="form-control daterange" value="2018-12-31">
                </div>
                <div class="form-group">
                    <label for="price" class="when">Price Range</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="400000">
                    <label>to</label>
                    <input type="number" class="form-control daterange" min="10000" max="100000000" value="800000">
                </div>
                <a href="/search" class="btn btn-primary" type="submit">Search</a>
            </form>
        </div>
            <div class="loginbox">
                <form onsubmit="gotodashboard()">
                    <i class="fas fa-times-circle"></i>
                    <div class="form-group logformgroup">
                      <input type="text" class="form-control logformcon" placeholder="Email">
                    </div>
                    <div class="form-group logformgroup">
                      <input type="text" class="form-control logformcon" placeholder="Password">
                    </div>
                    <a class="btn btn-primary logbutton" href="dashboard">Log in</a>
                </form>
        </div>
        <div class="carrot">
            <i class="fas fa-chevron-down"></i>
        </div>
